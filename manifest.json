{
  "description": "Insurance fees calculation for finfo.tw",
  "manifest_version": 2,
  "name": "finfo",
  "version": "1.0",
  "homepage_url": "https://github.com/mlouielu/finfo-web-extension",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
	  "run_at": "document_end",
      "matches": ["*://finfo.tw/assortments/*"],
      "js": ["finfo.js"]
    }
  ]

}
